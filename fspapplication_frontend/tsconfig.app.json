{
  "extends": "@vue/tsconfig/tsconfig.dom.json",
  "include": ["env.d.ts", "src/**/*", "src/**/*.vue"],
  "exclude": [
    "src/**/__tests__/*",
    "src/components/organisation/CompanyAddressCard.vue",
    "src/components/organisation/CompanyBrandingCard.vue",
    "src/components/organisation/CompanyDetailsCard.vue",
    "src/components/organisation/CompanyInfoCard.vue"
  ],
  "compilerOptions": {
    "composite": true,
    "noEmit": true,
    "tsBuildInfoFile": "./node_modules/.tmp/tsconfig.app.tsbuildinfo",
    "paths": {
      "@/*": ["./src/*"]
    }
  }
}
